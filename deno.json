{
  "tasks": {
    "dev": "deno run -A --unstable --watch main.ts",
    "start": "deno run -Ar --unstable main.ts",
    "deploy": "fly deploy",
    "status": "fly status"
  },
  "imports": {
    "@/": "./",
    "@hono": "jsr:@hono/hono@^4.4.0"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "@hono/jsx",
    "types": [
      "./types.d.ts"
    ]
  }
}
